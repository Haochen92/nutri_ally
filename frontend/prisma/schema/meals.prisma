model SavedMeals {
    userId         String
    meal_type      String
    meal_name      String   
    meal_items     Json
    meal_macros    Json @default("{}")
    
    createdAt      DateTime @default(now())
    updatedAt      DateTime @updatedAt    

    user User @relation(fields:[userId], references:[id], onDelete: Cascade)    

    @@id(name: "userId_meal_name", [userId, meal_name])   
}